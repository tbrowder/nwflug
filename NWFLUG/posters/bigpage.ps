} def

/header {
  gsave
  /Helvetica-Bold 24 selectfont
  4.25 i2p 11 0.3 sub i2p moveto (SECRET) 7 puttext
  4.25 i2p 0.3 i2p moveto (SECRET) 3 puttext

  grestore
} def

/fileref {
  gsave
  /Helvetica 12 selectfont
  6.5 i2p 11 .75 sub i2p moveto (FILE: _____________) show

  grestore
} def

